<html>
    <head>
       <title><%= view.json.title %></title>
       <%
       if(view.mainjson) {

                view.mainjson.css.forEach(function(css) {
                %>
                        <link rel="stylesheet" type="text/css" href="<%= css %>">
                <%
                })

                view.mainjson.js.forEach(function(js) {
                %>
                        <script  type="text/javascript" src="<%= js %>"></script>
                <%
                })

                view.mainjson.fonts.forEach(function(font) {
                %>
                        <link href="<%= font %>" rel="stylesheet">
                <%
                })
        }
        %>

        <%
       if(view.json) {

                view.json.css.forEach(function(css) {
                %>
                        <link rel="stylesheet" type="text/css" href="<%= css %>">
                <%
                })

                view.json.js.forEach(function(js) {
                %>
                        <script  type="text/javascript" src="<%= js %>"></script>
                <%
                })

                view.json.fonts.forEach(function(font) {
                %>
                        <link href="<%= font %>" rel="stylesheet">
                <%
                })
        }
        %>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <script type="text/javascript">
         	jQuery(document).ready(function($) {
         		$(".scroll").click(function(event){
         			event.preventDefault();
      			$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
         		});
         	});
         </script>
    </head>
    <body>

    <% view.files.headers.forEach((header) => { %>
            <%- include('../../' + header) %>
    <% }) %>

    <% view.files.filesToInclude.forEach((file) => { %>
            <%- include('../../' + file) %>
    <% }) %>
